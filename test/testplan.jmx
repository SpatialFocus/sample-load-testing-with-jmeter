            <stringProp name="scriptLanguage">groovy</stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="script">int startIndex = (${__threadNum} - 1) * ${number_of_loops}

for(int i = 1; i &lt;= ${number_of_loops}; i++)
{
	int customerIndex = startIndex + i
	log.info(&quot;Storing CustomerID #${customerIndex}: ${vars.get(&apos;customer_ids_&apos; + i)}&quot;)
	props.put(&apos;customer_ids_&apos; + customerIndex,  vars.get(&apos;customer_ids_&apos; + i))
}</stringProp>
          </JSR223PostProcessor>
          <hashTree/>
        </hashTree>
